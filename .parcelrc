{
    "extends": "@parcel/config-default",
    "transformers": {
        "*.{svg}": ["@parcel/transformer-svg"]
      },
      "optimizers": {
        "*.{svg}": ["@parcel/optimizer-svgo"]
      },
    "compressors": {
        "*.{html,css,js,svg,map}": [
            "...",
            "@parcel/compressor-gzip",
            "@parcel/compressor-brotli"
        ]
    }
}